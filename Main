#include<stdio.h>
int main()
{
int no;
printf("Enter Student Marks : ");
scanf("%d",&no);
if(no>=85 && no<=100)
printf("Grade A");
else if(no>=70 && no <=84)
printf("Grade B");
else if(no>=55 && no <=69)
printf("Grade C");
else if(no>=40 && no <=54)
printf("Grade D");
else if(no<40)
printf("Grade F");

return 0;
}
